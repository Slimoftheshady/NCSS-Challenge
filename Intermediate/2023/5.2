data = {}
li = []
with open('midi.txt') as f:
  for line in f:
    key, value = line.split()
    data[key] = value
a = input('Secret Message: ').split()
for i in range(len(a)):
  if a[i] in data:
      li.append(data[a[i]])

print("Translation: " + " ".join(li))
