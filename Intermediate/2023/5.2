lists = []
a = input("Enter an award file: ")
with open(a) as f:
  for line in f:
    line = line.strip()
    if line not in lists:
      lists.append(line)

lists.sort()
print("And the nominees are...")
for i in lists:
  print(f"🎬 {i}")
